#include<stdio.h>
int main(){
    int p, q, r;

    printf("Enter the number- ");
    scanf("%d %d %d", &p, &q, &r);
    if (p > q & p > q) {
    printf("The greatest number is: %d", p);
    } else if (q > p & q > r) {
    printf("The greatest number is: %d", q);
    } 
    else {
    printf("The greatest number is: %d", r);
    }
    return 0;
}
