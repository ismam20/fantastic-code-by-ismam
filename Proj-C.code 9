#include<stdio.h>
int main(){
    int p, q, r;

    printf("Enter the number- ");
    scanf("%d %d %d", &p,&q,&r);
    if (p < q & p < r) {
    printf("The smallest number is: %d\n", p);
    } else if (q < p & q < r) {
    printf("The smallest number is: %d\n", q);
    } 
    else {
    printf("The smallest number is: %d\n", r);
    }
    return 0;
}
